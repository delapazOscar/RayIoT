<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="daisies_migrate_payment_methods">
            <field name="name">Rayiot: Determinar estado del dispositivo</field>
            <field name="model_id" ref="model_dais_user"/>
            <field name="state">code</field>
            <field name="code">model.cron_define_device_state()</field>
            <field name="active">False</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(datetime.utcnow() + timedelta(days=0)).strftime('%Y-%m-%d 23:55:00')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>
    </data>
</odoo>